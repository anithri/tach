<?xml version="1.0"?>
<templateSet group="tach-word-break">
  <template name=".word-normal" value="/* tach-word-break: .word-normal */&#10;word-break: normal;&#10;$END$" description=".word-normal" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".word-wrap" value="/* tach-word-break: .word-wrap */&#10;word-break: break-all;&#10;$END$" description=".word-wrap" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".word-nowrap" value="/* tach-word-break: .word-nowrap */&#10;word-break: keep-all;&#10;$END$" description=".word-nowrap" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
