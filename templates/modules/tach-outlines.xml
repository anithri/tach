<?xml version="1.0"?>
<templateSet group="tach-outlines">
  <template name=".outline" value="outline: 1px solid;&#10;$END$" description=".outline" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".outline-transparent" value="outline: 1px solid transparent;&#10;$END$" description=".outline-transparent" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".outline-0" value="outline: 0;&#10;$END$" description=".outline-0" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
