<?xml version="1.0"?>
<templateSet group="tach-clears">
  <template name=".cf" value="*zoom: 1;&#10;$END$" description=".cf" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".cl" value="clear: left;&#10;$END$" description=".cl" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".cr" value="clear: right;&#10;$END$" description=".cr" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".cb" value="clear: both;&#10;$END$" description=".cb" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".cn" value="clear: none;&#10;$END$" description=".cn" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
