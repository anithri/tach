<?xml version="1.0"?>
<templateSet group="tach-white-space">
  <template name=".ws-normal" value="white-space: normal;&#10;$END$" description=".ws-normal" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".nowrap" value="white-space: nowrap;&#10;$END$" description=".nowrap" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".pre" value="white-space: pre;&#10;$END$" description=".pre" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
