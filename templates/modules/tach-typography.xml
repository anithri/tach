<?xml version="1.0"?>
<templateSet group="tach-typography">
  <template name=".measure" value="max-width: var(--measure);&#10;$END$" description=".measure" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".measure-wide" value="max-width: var(--measure-wide);&#10;$END$" description=".measure-wide" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".measure-narrow" value="max-width: var(--measure-narrow);&#10;$END$" description=".measure-narrow" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".indent" value="text-indent: 1em;&#10;$END$" description=".indent" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".small-caps" value="font-variant: small-caps;&#10;$END$" description=".small-caps" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".truncate" value="white-space: nowrap;&#10;$END$" description=".truncate" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
