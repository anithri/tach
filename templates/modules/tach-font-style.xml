<?xml version="1.0"?>
<templateSet group="tach-font-style">
  <template name=".i" value="font-style: italic;&#10;$END$" description=".i" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".fs-normal" value="font-style: normal;&#10;$END$" description=".fs-normal" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
