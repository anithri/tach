<?xml version="1.0"?>
<templateSet group="tach-text-transform">
  <template name=".ttc" value="text-transform: capitalize; /* .ttc */&#10;$END$" description=".ttc" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".ttl" value="text-transform: lowercase; /* .ttl */&#10;$END$" description=".ttl" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".ttu" value="text-transform: uppercase; /* .ttu */&#10;$END$" description=".ttu" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".ttn" value="text-transform: none; /* .ttn */&#10;$END$" description=".ttn" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
