<?xml version="1.0"?>
<templateSet group="tach-text-decoration">
  <template name=".strike" value="text-decoration: line-through; /* .strike */&#10;$END$" description=".strike" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".underline" value="text-decoration: underline; /* .underline */&#10;$END$" description=".underline" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
  <template name=".no-underline" value="text-decoration: none; /* .no-underline */&#10;$END$" description=".no-underline" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS_DECLARATION_BLOCK" value="true"/>
    </context>
  </template>
</templateSet>
